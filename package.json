        "iconThemes": [
            {
                "id": "tim-odoo-icons",
                "label": "Tim Odoo Icon",
                "path": "./icons.json"
            }
        ],
        "keybindings": [],
        "languages": [
            {
                "id": "javascript",
                "aliases": [
                    "JavaScript",
                    "js"
                ],
                "extensions": [
                    ".js"
                ],
                "filenames": [
                    "jsconfig.json"
                ],
                "filenamePatterns": [
                    "*.js"
                ],
                "mimetypes": [
                    "application/javascript",
                    "text/javascript"
                ]
            },
            {
                "id": "typescript",
                "aliases": [
                    "TypeScript",
                    "ts"
                ],
                "extensions": [
                    ".ts",
                    ".tsx"
                ],
                "filenames": [
                    "tsconfig.json"
                ],
                "filenamePatterns": [
                    "*.ts",
                    "*.tsx"
                ],
                "mimetypes": [
                    "application/typescript",
                    "text/typescript"
                ]
            },
            {
                "id": "python",
                "aliases": [
                    "Python",
                    "py"
                ],
                "extensions": [
                    ".py"
                ],
                "filenames": [
                    "requirements.txt",
                    "setup.py"
                ],
                "filenamePatterns": [
                    "*.py"
                ],
                "mimetypes": [
                    "text/x-python"
                ]
            },
            {
                "id": "xml",
                "aliases": [
                    "XML"
                ],
                "extensions": [
                    ".xml"
                ],
                "filenames": [
                    "settings.xml"
                ],
                "filenamePatterns": [
                    "*.xml"
                ],
                "mimetypes": [
                    "application/xml",
                    "text/xml"
                ]
            },
            {
                "id": "csv",
                "aliases": [
                    "CSV"
                ],
                "extensions": [
                    ".csv"
                ],
                "filenames": [
                    "data.csv"
                ],
                "filenamePatterns": [
                    "*.csv"
                ],
                "mimetypes": [
                    "text/csv"
                ]
            }
        ],
        "themes": [
            {
                "label": "Odoo Dark",
                "id": "odoo.dark",
                "uiTheme": "vs-dark",
                "path": "./themes/odoo.dark.json"
            }
        ],
        "breakpoints": [
            {
                "language": "javascript"
            },
            {
                "language": "javascriptreact"
            },
            {
                "language": "typescript"
            },
            {
                "language": "typescriptreact"
            },
            {
                "language": "python"
            },
            {
                "language": "html"
            },
            {
                "language": "css"
            },
            {
                "language": "json"
            }
        ],
        "commands": []
    },
    "extensionDependencies": [
        "esbenp.prettier-vscode",
        "dbaeumer.vscode-eslint",
        "ms-python.python",
        "ms-python.autopep8",
        "ms-python.vscode-pylance",
        "mechatroner.rainbow-csv",
        "ms-python.debugpy",
        "dotjoshjohnson.xml",
        "formulahendry.auto-rename-tag",
        "formulahendry.auto-close-tag"
    ],
    "icon": "assets/brand/icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/buiductuan-dev/snippets.git"
    },
    "keywords": [
        "odoo",
        "snippets",
        "python",
        "xml",
        "javascript",
        "csv"
    ],
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^8.10.25",
        "@types/vscode": "^1.51.0",
        "tslint": "^5.16.0",
        "typescript": "^3.4.5"
    },
    "languages": [
        {
            "id": "javascript",
            "extensions": [
                ".js",
                ".jsx",
                ".ts",
                ".tsx"
            ]
        },
        {
            "id": "html",
            "extensions": [
                ".html"
            ]
        },
        {
            "id": "python",
            "extensions": ".py"
        },
        {
            "id": "xml",
            "extensions": ".xml"
        },
        {
            "id": "csv",
            "extensions": ".csv"
        }
    ]
}
